#include <stdio.h>

int main()
{
    int marks[10];
    int n, i, j, temp;

    // Input number of students
    printf("Enter number of students: ");
    scanf("%d", &n);

    // Input marks
    printf("Enter marks of students:\n");
    for(i = 0; i < n; i++)
    {
        printf("Student %d: ", i + 1);
        scanf("%d", &marks[i]);
    }

    // Sorting marks in ascending order (Bubble Sort)
    for(i = 0; i < n - 1; i++)
    {
        for(j = 0; j < n - i - 1; j++)
        {
            if(marks[j] > marks[j + 1])
            {
                temp = marks[j];
                marks[j] = marks[j + 1];
                marks[j + 1] = temp;
            }
        }
    }

    // Display sorted marks
    printf("\nSorted Marks (Ascending Order):\n");
    for(i = 0; i < n; i++)
    {
        printf("%d ", marks[i]);
    }

    return 0;
}
