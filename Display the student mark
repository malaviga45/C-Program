#include <stdio.h>

int main()
{
    int marks[50];
    int i;
    int n;

    // Display program title
    printf("Student Marks Storage Program\n");
    printf("-----------------------------\n");

    // Input number of students
    printf("Enter number of students: ");
    scanf("%d", &n);

    // Input marks of students
    for (i = 0; i < n; i++)
    {
        printf("Enter marks of student %d: ", i + 1);
        scanf("%d", &marks[i]);
    }

    // Display marks of students
    printf("\nMarks of Students\n");
    printf("-----------------\n");

    for (i = 0; i < n; i++)
    {
        printf("Student %d: %d\n", i + 1, marks[i]);
    }

    return 0;
}
